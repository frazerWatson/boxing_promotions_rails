<h1>Fighters</h1>
<% if user_signed_in? %>
  <%= link_to "Sign out", destroy_user_session_path, method: :delete %>
<% else %>
  <%= link_to "Sign in", new_user_session_path %>
  <%= link_to "Register as a fighter", new_user_registration_path %>
<% end %>
<% if @fighters.any? %>
<ul>
<% @fighters.each do |fighter| %>
<li>
  <%= fighter.name %>
  <%= fighter.avatar %>
  <%= fighter.height %>
  <%= fighter.weight %>
 
</li>
<% end %>
</ul>
<% else %>
<p>No fighters yet</p>
<% end %>